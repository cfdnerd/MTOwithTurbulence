// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

raa0                1e-4;
voluse              0.4;
mma_init            0.3;
mma_dec             0.7;
mma_inc             1.1;
movlim              0.2;
filterR             3.0;
PowerDiss0          1e-12;
PowerDissMax        10;
PowerDissRelax      25;
GeoDim              2;
fluid_area          yes;
solid_area          no;
test_area           yes;
Pnorm               300;
objFunction         1;
// Adjoint turbulence coupling: 2 = two passes of ka,omegaa,Ua (iterative); 1 = single pass
nAdjTurbPasses      2;
// Turbulence sensitivity damping coeff (0=off; use if Rk/Romega have -c*alpha*k/omega in solid)
turbSensDamp        0;

