FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


kf                           kf [1 1 -3 -1 0 0 0] 0.61;
ks                           ks [1 1 -3 -1 0 0 0] 237;
rho                          rho [1 -3 0 0 0 0 0] 1000;
cp                           cp [0 2 -2 -1 0 0 0] 4180;
heatSource                   heatSource [1 -1 -3 0 0 0 0] 1e8;
Q                            Q [0 0 -1 1 0 0 0] 1.0;
DDHS                         no;
Prt                          0.85;

// heatSource: volumetric heat source [W/m3], used in primal energy eqn
// Q: adjoint heat source [K/s], used in adjoint Tb eqn (objFunction 1)
// rhoc = rho*cp computed from rho and cp
// ************************************************************************* //
