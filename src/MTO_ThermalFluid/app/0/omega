/*--------------------------------*- C++ -*----------------------------------*\
|  Turbulent specific dissipation for kOmegaSST
\*---------------------------------------------------------------------------*/
FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 200;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 200;  // omega = k^0.5/(Cmu^0.25*L), L=0.07*Dh, Dh=8mm
    }

    outlet
    {
        type            inletOutlet;
        inletValue      uniform 200;
    }

    Wall
    {
        type            omegaWallFunction;
        value           uniform 200;
    }

    right
    {
        type            symmetry;
    }

    frontAndBack
    {
        type            empty;
    }

}

// ************************************************************************* //
