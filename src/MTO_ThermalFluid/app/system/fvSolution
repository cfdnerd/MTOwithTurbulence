FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(p|pa|pb|pc)"
    {
        solver          GAMG;
        smoother        DICGaussSeidel;
        tolerance       1e-8;
        relTol          0.01;
        maxIter         500;
        nCellsInCoarsestLevel 10;
    }

    "(U|Ua|Ub|Uc|k|omega|ka|omegaa|epsilon)"
    {
        solver          smoothSolver;
        smoother        GaussSeidel;
        nSweeps         4;
        tolerance       1e-8;
        relTol          0.01;
    }
    T
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-6;
        relTol          0.01;
    }
    "(Ta|Tb)"
    {
        solver          GAMG;
        smoother        DICGaussSeidel;
        tolerance       1e-7;
        relTol          0.001;
        maxIter         2000;
        nCellsInCoarsestLevel 10;
    }
     "(xp|fsensMeanT|gsensVol|gsensPowerDiss)"
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-8;
        relTol          0;
    }
}

SIMPLE
{
    nNonOrthogonalCorrectors 1;
 //   nCorrectors 2;
// nOuterCorrectors 10;
    pRefCell 0;
    pRefValue 0;

}

relaxationFactors
{
    fields
    {
        "(p|pa|pb|pc)"     0.2;
        alpha           0.1;
    }
    equations
    {
        "(U|Ua|Ub|Uc)"     0.7;
        "(k|omega|ka|omegaa|epsilon)"   0.7;
        T                 0.9;
        "(Ta|Tb)"         0.9;
    }
}
constraintFunctionDict_flow
{
    numberConstraintPatches      2;
    constraintPatchesNames       (inlet outlet);
}
